{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/(user)/(site)/(shop)/shop/[slug]/Gallery.jsx", "src/app/(user)/(site)/(shop)/shop/[slug]/page.jsx", "src/app/(user)/(site)/(shop)/shop/layout.jsx", "src/app/(user)/(site)/(shop)/shop/loading.jsx", "src/app/(user)/(site)/(shop)/shop/page.jsx", "src/app/(user)/(site)/(shop)/shop/SiderBar.jsx", "src/services/QueryConverter.js", "src/services/toStringCookie.ts"],
  "exclude": ["node_modules"]
}
